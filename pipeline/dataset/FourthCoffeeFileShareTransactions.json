{
	"name": "FourthCoffeeFileShareTransactions",
	"properties": {
		"linkedServiceName": {
			"referenceName": "linkedService2",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"updaredDate": {
				"type": "String",
				"defaultValue": "2017-01-01"
			}
		},
		"type": "FileShare",
		"typeProperties": {
			"format": {
				"type": "TextFormat",
				"columnDelimiter": ",",
				"rowDelimiter": "",
				"nullValue": "\\N",
				"treatEmptyAsNull": true,
				"skipLineCount": 0,
				"firstRowAsHeader": false
			},
			"fileName": {
				"value": "@concat(dataset().updaredDate, '.csv')",
				"type": "Expression"
			}
		}
	}
}