{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "southridge-exportdata"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"CloudSales_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CloudSales'"
		},
		"CloudStreaming_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CloudStreaming'"
		},
		"CosmosCatalogDbConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosCatalogDbConnection'"
		},
		"catalog_movies_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'catalog_movies'"
		},
		"southridge_7_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'southridge_7_adls'"
		},
		"southridge_7_dls_catalog_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'southridge_7_dls_catalog'"
		},
		"Catalog_movies_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@dataset().filename"
		},
		"Catalog_movies_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "@dataset().filepath"
		},
		"southridge_7_adls_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@dataset().filename"
		},
		"southridge_7_adls_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "@dataset().filepath"
		},
		"catalog_movies_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://southridge7.dfs.core.windows.net"
		},
		"southridge_7_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://southridge7.dfs.core.windows.net"
		},
		"southridge_7_dls_catalog_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://southridge7.dfs.core.windows.net"
		},
		"trigger_addresses_properties_southridge-cloudsales_parameters_filepath": {
			"type": "string",
			"defaultValue": "cloudsales"
		},
		"trigger_addresses_properties_southridge-cloudsales_parameters_filename": {
			"type": "string",
			"defaultValue": "addresses.csv"
		},
		"trigger_addresses_properties_southridge-cloudsales_parameters_table": {
			"type": "string",
			"defaultValue": "[concat('[', 'dbo].[Addresses]')]"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/southridge-catalog-movies')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Movies",
				"activities": [
					{
						"name": "Cosmos Connection",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 3,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "DocumentDbCollectionSource",
								"nestingSeparator": "."
							},
							"sink": {
								"type": "AzureBlobFSSink"
							},
							"enableStaging": false
						},
						"inputs": [
							{
								"referenceName": "CosmosSqlApi",
								"type": "DatasetReference",
								"parameters": {
									"filename": "movies",
									"filepath": "catalog"
								}
							}
						],
						"outputs": [
							{
								"referenceName": "Catalog_movies",
								"type": "DatasetReference",
								"parameters": {
									"filename": {
										"value": "@pipeline().parameters.filename",
										"type": "Expression"
									},
									"filepath": {
										"value": "@pipeline().parameters.filepath",
										"type": "Expression"
									}
								}
							}
						]
					}
				],
				"parameters": {
					"filename": {
						"type": "String",
						"defaultValue": "movies.json"
					},
					"filepath": {
						"type": "String",
						"defaultValue": "catalog"
					}
				},
				"folder": {
					"name": "Catalog"
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/CosmosSqlApi')]",
				"[concat(variables('factoryId'), '/datasets/Catalog_movies')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/southridge-cloudsales')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "southridge-cloudsales",
				"activities": [
					{
						"name": "CopyDataFromSource",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 3,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "SqlSource"
							},
							"sink": {
								"type": "AzureBlobFSSink"
							},
							"enableStaging": false
						},
						"inputs": [
							{
								"referenceName": "CloudSales",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "southridge_7_adls",
								"type": "DatasetReference",
								"parameters": {
									"filename": {
										"value": "@pipeline().parameters.filename",
										"type": "Expression"
									},
									"filepath": {
										"value": "@pipeline().parameters.filepath",
										"type": "Expression"
									}
								}
							}
						]
					}
				],
				"parameters": {
					"filepath": {
						"type": "String",
						"defaultValue": "cloudsales"
					},
					"filename": {
						"type": "String",
						"defaultValue": "addresses.csv"
					},
					"table": {
						"type": "String",
						"defaultValue": "[concat('[', 'dbo].[Addresses]')]"
					}
				},
				"folder": {
					"name": "CloudSales"
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/southridge_7_adls')]",
				"[concat(variables('factoryId'), '/datasets/CloudSales')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/southridge-cloudstreaming')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "southridge-cloudstreaming",
				"activities": [
					{
						"name": "CloudStreaming",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 3,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "SqlSource"
							},
							"sink": {
								"type": "AzureBlobFSSink"
							},
							"enableStaging": false
						},
						"inputs": [
							{
								"referenceName": "CloudStreaming",
								"type": "DatasetReference",
								"parameters": {
									"table": {
										"value": "@pipeline().parameters.table",
										"type": "Expression"
									}
								}
							}
						],
						"outputs": [
							{
								"referenceName": "southridge_7_adls",
								"type": "DatasetReference",
								"parameters": {
									"filename": {
										"value": "@pipeline().parameters.filename",
										"type": "Expression"
									},
									"filepath": {
										"value": "@pipeline().parameters.filepath",
										"type": "Expression"
									}
								}
							}
						]
					}
				],
				"parameters": {
					"filepath": {
						"type": "String",
						"defaultValue": "cloudstreaming"
					},
					"filename": {
						"type": "String",
						"defaultValue": "customers.csv"
					},
					"table": {
						"type": "String",
						"defaultValue": "[concat('[', 'dbo].[Customers]')]"
					}
				},
				"folder": {
					"name": "CloudStreaming"
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/southridge_7_adls')]",
				"[concat(variables('factoryId'), '/datasets/CloudStreaming')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Catalog_movies')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "catalog_movies",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"filename": {
						"type": "String"
					},
					"filepath": {
						"type": "String"
					}
				},
				"annotations": [],
				"type": "AzureBlobFSFile",
				"typeProperties": {
					"format": {
						"type": "JsonFormat",
						"filePattern": "arrayOfObjects"
					},
					"fileName": {
						"value": "[parameters('Catalog_movies_properties_typeProperties_fileName')]",
						"type": "Expression"
					},
					"folderPath": {
						"value": "[parameters('Catalog_movies_properties_typeProperties_folderPath')]",
						"type": "Expression"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/catalog_movies')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/CloudSales')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "CloudSales",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"typeProperties": {
					"tableName": "[concat('[', 'dbo].[Addresses]')]"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/CloudSales')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/CloudStreaming')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "CloudStreaming",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"table": {
						"type": "String"
					}
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"typeProperties": {
					"tableName": {
						"value": "@dataset().table",
						"type": "Expression"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/CloudStreaming')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/CosmosSqlApi')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "CosmosCatalogDbConnection",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"filename": {
						"type": "String"
					},
					"filepath": {
						"type": "String"
					}
				},
				"annotations": [],
				"type": "DocumentDbCollection",
				"typeProperties": {
					"collectionName": "movies"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/CosmosCatalogDbConnection')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/southridge_7_adls')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "southridge_7_adls",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"filename": {
						"type": "String"
					},
					"filepath": {
						"type": "String"
					}
				},
				"annotations": [],
				"type": "AzureBlobFSFile",
				"typeProperties": {
					"format": {
						"type": "TextFormat",
						"columnDelimiter": ",",
						"rowDelimiter": "",
						"nullValue": "\\N",
						"treatEmptyAsNull": true,
						"skipLineCount": 0,
						"firstRowAsHeader": false
					},
					"fileName": {
						"value": "[parameters('southridge_7_adls_properties_typeProperties_fileName')]",
						"type": "Expression"
					},
					"folderPath": {
						"value": "[parameters('southridge_7_adls_properties_typeProperties_folderPath')]",
						"type": "Expression"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/southridge_7_adls')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/southridge_catalog_movies')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "southridge_catalog_movies",
				"linkedServiceName": {
					"referenceName": "CosmosCatalogDbConnection",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"filename": {
						"type": "String"
					},
					"filepath": {
						"type": "String"
					}
				},
				"annotations": [],
				"type": "DocumentDbCollection",
				"typeProperties": {
					"collectionName": "movies"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/CosmosCatalogDbConnection')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlobStorage1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "testBlobStorage",
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('AzureBlobStorage1_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/CloudSales')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('CloudSales_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/CloudStreaming')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('CloudStreaming_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/CosmosCatalogDbConnection')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "CosmosDb",
				"typeProperties": {
					"connectionString": "[parameters('CosmosCatalogDbConnection_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/catalog_movies')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('catalog_movies_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('catalog_movies_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/southridge_7_adls')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('southridge_7_adls_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('southridge_7_adls_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/southridge_7_dls_catalog')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('southridge_7_dls_catalog_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('southridge_7_dls_catalog_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/trigger_addresses')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Started",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "southridge-cloudsales",
							"type": "PipelineReference"
						},
						"parameters": {
							"filepath": "[parameters('trigger_addresses_properties_southridge-cloudsales_parameters_filepath')]",
							"filename": "[parameters('trigger_addresses_properties_southridge-cloudsales_parameters_filename')]",
							"table": "[parameters('trigger_addresses_properties_southridge-cloudsales_parameters_table')]"
						}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Minute",
						"interval": 1,
						"startTime": "2019-03-26T19:32:00.000Z",
						"timeZone": "UTC"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/pipelines/southridge-cloudsales')]"
			]
		}
	]
}