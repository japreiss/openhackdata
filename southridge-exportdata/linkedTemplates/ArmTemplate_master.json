{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "southridge-exportdata"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"CloudSales_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CloudSales'"
		},
		"southridge_7_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'southridge_7_adls'"
		},
		"southridge_7_adls_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@dataset().filename"
		},
		"southridge_7_adls_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "@dataset().filepath"
		},
		"southridge_7_adls1_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@dataset().filename"
		},
		"southridge_7_adls1_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "@dataset().filepath"
		},
		"southridge_7_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://southridge7.dfs.core.windows.net"
		},
		"southridge_7_adls2_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@dataset().filename"
		},
		"southridge_7_adls2_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "@dataset().filepath"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"CloudSales_connectionString": {
						"value": "[parameters('CloudSales_connectionString')]"
					},
					"southridge_7_adls_accountKey": {
						"value": "[parameters('southridge_7_adls_accountKey')]"
					},
					"southridge_7_adls_properties_typeProperties_url": {
						"value": "[parameters('southridge_7_adls_properties_typeProperties_url')]"
					},
					"southridge_7_adls_properties_typeProperties_fileName": {
						"value": "[parameters('southridge_7_adls_properties_typeProperties_fileName')]"
					},
					"southridge_7_adls_properties_typeProperties_folderPath": {
						"value": "[parameters('southridge_7_adls_properties_typeProperties_folderPath')]"
					},
					"southridge_7_adls1_properties_typeProperties_fileName": {
						"value": "[parameters('southridge_7_adls1_properties_typeProperties_fileName')]"
					},
					"southridge_7_adls1_properties_typeProperties_folderPath": {
						"value": "[parameters('southridge_7_adls1_properties_typeProperties_folderPath')]"
					},
					"southridge_7_adls2_properties_typeProperties_fileName": {
						"value": "[parameters('southridge_7_adls2_properties_typeProperties_fileName')]"
					},
					"southridge_7_adls2_properties_typeProperties_folderPath": {
						"value": "[parameters('southridge_7_adls2_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}