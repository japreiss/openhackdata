{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "southridge-exportdata"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"CloudSales_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CloudSales'"
		},
		"CloudStreaming_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CloudStreaming'"
		},
		"CosmosCatalogDbConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosCatalogDbConnection'"
		},
		"VanArsdelLtdOnPrem_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'VanArsdelLtdOnPrem'"
		},
		"catalog_movies_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'catalog_movies'"
		},
		"linkedService1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'linkedService1'"
		},
		"southridge_7_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'southridge_7_adls'"
		},
		"southridge_7_dls_catalog_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'southridge_7_dls_catalog'"
		},
		"Catalog_movies_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@dataset().filename"
		},
		"Catalog_movies_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "@dataset().filepath"
		},
		"southridge_7_adls_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@dataset().filename"
		},
		"southridge_7_adls_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "@dataset().filepath"
		},
		"catalog_movies_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://southridge7.dfs.core.windows.net"
		},
		"linkedService1_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "openhackadmin"
		},
		"southridge_7_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://southridge7.dfs.core.windows.net"
		},
		"southridge_7_dls_catalog_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://southridge7.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"CloudSales_connectionString": {
						"value": "[parameters('CloudSales_connectionString')]"
					},
					"CloudStreaming_connectionString": {
						"value": "[parameters('CloudStreaming_connectionString')]"
					},
					"CosmosCatalogDbConnection_connectionString": {
						"value": "[parameters('CosmosCatalogDbConnection_connectionString')]"
					},
					"catalog_movies_accountKey": {
						"value": "[parameters('catalog_movies_accountKey')]"
					},
					"southridge_7_adls_accountKey": {
						"value": "[parameters('southridge_7_adls_accountKey')]"
					},
					"southridge_7_dls_catalog_accountKey": {
						"value": "[parameters('southridge_7_dls_catalog_accountKey')]"
					},
					"VanArsdelLtdOnPrem_connectionString": {
						"value": "[parameters('VanArsdelLtdOnPrem_connectionString')]"
					},
					"linkedService1_password": {
						"value": "[parameters('linkedService1_password')]"
					},
					"catalog_movies_properties_typeProperties_url": {
						"value": "[parameters('catalog_movies_properties_typeProperties_url')]"
					},
					"southridge_7_adls_properties_typeProperties_url": {
						"value": "[parameters('southridge_7_adls_properties_typeProperties_url')]"
					},
					"southridge_7_dls_catalog_properties_typeProperties_url": {
						"value": "[parameters('southridge_7_dls_catalog_properties_typeProperties_url')]"
					},
					"Catalog_movies_properties_typeProperties_fileName": {
						"value": "[parameters('Catalog_movies_properties_typeProperties_fileName')]"
					},
					"Catalog_movies_properties_typeProperties_folderPath": {
						"value": "[parameters('Catalog_movies_properties_typeProperties_folderPath')]"
					},
					"southridge_7_adls_properties_typeProperties_fileName": {
						"value": "[parameters('southridge_7_adls_properties_typeProperties_fileName')]"
					},
					"southridge_7_adls_properties_typeProperties_folderPath": {
						"value": "[parameters('southridge_7_adls_properties_typeProperties_folderPath')]"
					},
					"linkedService1_properties_typeProperties_userId": {
						"value": "[parameters('linkedService1_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}